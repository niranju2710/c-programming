#include <stdio.h>
#include <string.h>

int main() {
    char str[1000];
    int length;

    // Input string
    printf("Enter a string: ");
    scanf(" %[^\n]s", str); // read string including spaces

    // Find length
    length = strlen(str);

    // Display result
    printf("Length of the string is: %d\n", length);

    return 0;
}