#include <stdio.h>

int main() {
    int n, i;
    
    printf("Enter number of students: ");
    scanf("%d", &n);

    int marks[n];  // Array to store marks

    // Input marks
    for (i = 0; i < n; i++) {
        printf("Enter marks of student %d: ", i + 1);
        scanf("%d", &marks[i]);
    }

    // Display marks
    printf("\nMarks of students:\n");
    for (i = 0; i < n; i++) {
        printf("Student %d: %d\n", i + 1, marks[i]);
    }

    return 0;
}